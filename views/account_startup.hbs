<head>
   <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
   <link rel="stylesheet" href="/css/Account.css">
</head>

<body>
    {{> _header}}
   <div class="container">
       <div class="main">
           </div>
           <div class="row">
             <div class="col-md-4 mt-1">
                 <div class="card text-center sidebar">
                   <div class="card-body">
                     <img src="/images/broker.png" class="rounded-circle" width="150">
                     <div class="mt-3">
 					     <h3>Account type</h3>                         
                    </div>
                  </div>
                </div>
             </div>
             <div class="col-md-8 mt-1">
                  <div class="card mb-3 content">
                     <h1 class="a-3 pt-3">About</h1>
                     <div class="card-body">
                          <div class="row">
                             <div class="col-md-3">
                               <h5>Startup Name</h5>
                             </div>
                             <div class="col-md-9 text-secondary">
                                 {startupName}
                             </div>
                         </div>
                         <hr>
                         <div class="row">
                            <div class="col-md-3">
                              <h5>Startup Email</h5>
                            </div>
                            <div class="col-md-9 text-secondary">
                               {startupEmail}
                            </div>
                        </div>
                        <hr>
                        <div class="row">
                           <div class="col-md-3">
                                <h5>Startup CIN</h5>
                           </div>
                           <div class="col-md-9 text-secondary">
                              {startupCIN}
                           </div>
                        </div>
                        <hr>
                        <div class="row">
                             <div class="col-md-3">
                                  <h5>Startup stage</h5>
                             </div>
                             <div class="col-md-9 text-secondary">
  							      {startupStage}
                             </div>
                         </div>
						 <hr>
                        <div class="row">
                             <div class="col-md-3">
                                  <h5>Startup Nature</h5>
                             </div>
                             <div class="col-md-9 text-secondary">
  							      {startupNature}
                             </div>
                         </div>
						 <hr>
                        <div class="row">
                             <div class="col-md-3">
                                  <h5>Startup Website Link</h5>
                             </div>
                             <div class="col-md-9 text-secondary">
  							      <a href="https://play.google.com/store/apps/details?id=com.goflipr.flipr&hl=en_IN&gl=US">{startupWebsiteLink}</a>
                             </div>
                         </div> 
						 <hr>
                        <div class="row">
                             <div class="col-md-3">
                                  <h5>Startup Details</h5>
                             </div>
                             <div class="col-md-9 text-secondary">
  							       {startupDetails}
                             </div>
                         </div>
						 <hr>
                     </div>
                 </div>
                 
               </div>
           </div>
           <br><br>
